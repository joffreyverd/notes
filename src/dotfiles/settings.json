{
    "breadcrumbs.enabled": false,
    "php-cs-fixer.lastDownload": 1580936491940,
    "window.zoomLevel": 1,
    "files.insertFinalNewline": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "json.schemas": [],
    "files.associations": {
        "Dockerfile.*": "dockerfile",
        "*.js.php": "javascript",
        "*.css.php": "css"
    },
    "telemetry.enableTelemetry": false,
    "eslint.alwaysShowStatus": true,
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "'Fira Code'",
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.rules": {
        "@PSR2": true,
        "array_indentation": true,
        "array_syntax": {
            "syntax": "short"
        },
        "binary_operator_spaces": {
            "align_double_arrow": false,
            "align_equals": false
        },
        "blank_line_after_opening_tag": true,
        "cast_spaces": true,
        "compact_nullable_typehint": true,
        "concat_space": {
            "spacing": "one"       
        },
        "declare_equal_normalize" : true,
        "function_typehint_space": true,
        "lowercase_cast": true,
        "method_separation": true,
        "new_with_braces": true,
        "no_empty_statement": true,
        "no_blank_lines_after_class_opening": true,
        "no_leading_import_slash": true,
        "no_leading_namespace_whitespace": true,
        "no_whitespace_before_comma_in_array": true,
        "no_whitespace_in_blank_line": true,
        "return_type_declaration": {
            "space_before": "none"
        },
        "short_scalar_cast": true,
        "single_trait_insert_per_statement": true,
        "ternary_operator_spaces": true,
        "trim_array_spaces": true,
        "unary_operator_spaces": true
    },
    "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.wordWrap": "off",
    "workbench.activityBar.visible": true,
    "[json]": {
    
    },
    "yaml.schemas": {
        "file:///Users/jverd/.vscode/extensions/atlassian.atlascode-2.8.2/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.jqlList": [
        {
            "id": "e51b89a4-6205-4757-9e9f-b5560691213c",
            "enabled": true,
            "name": "My clinflows Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "ff2f625a-e8b4-4954-892e-519eb1fde859",
            "monitor": true
        }
    ],
    "files.autoSave": "onWindowChange",
    "php-cs-fixer.onsave": true
}
